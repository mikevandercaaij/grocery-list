<%- include("header") %>

<div class="box" id="heading">
    <h1><%= listTitle %></h1>
</div>

<div class="box">
    <% listItems.forEach(el => { %>
    <div class="item">
        <input type="checkbox" />
        <p><%= el %></p>
    </div>
    <% }) %>
    <form class="item" action="/" method="post">
        <input placeholder="New Item" name="newItem" type="text" autocomplete="off" />
        <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>
</div>

<%- include("footer") %>
